{
  "domain": "Teeking.com",
  "TLS": false,
  "docker": {
    "fabricTag": "1.1.0",
    "thirdPartyTag": "0.4.6",
    "network": "delphiNetwork",
    "volumes": {
      "CONFIGTX": {
        "dir": "/home/fabric2/Documents/delphi-fabric/config/configtx"
      },
      "MSPROOT": {
        "dir": "/home/fabric2/Documents/delphi-fabric/config/crypto-config/"
      }
    }
  },
  "orderer": {
    "type": "solo",
    "genesis_block": {
      "file": "tk.block",
      "profile": "TeekingGenesis"
    },
    "solo": {
      "container_name": "orderContainerName",
      "portMap": {
        "7050": 7050
      }
    },
    "kafka": {
      "M": 2,
      "N": 3,
      "rule": "1<M<N<K,K>=4,Orderer.AbsoluteMaxBytes < replica.fetch.max.bytes <= message.max.bytes",
      "zookeepers": {
        "zookeeper0": {
          "MY_ID": 0
        },
        "zookeeper1": {
          "MY_ID": 1
        },
        "zookeeper2": {
          "MY_ID": 2
        }
      },
      "kafkas": {
        "kafka0": {
          "BROKER_ID": 0
        },
        "kafka1": {
          "BROKER_ID": 1
        },
        "kafka2": {
          "BROKER_ID": 2
        },
        "kafka3": {
          "BROKER_ID": 3
        }
      },
      "orderers": {
        "orderer0": {
          "swarm": {
            "constraints": [
              "node.role==leader"
            ]
          },
          "portMap": {
            "7050": 7050
          }
        },
        "orderer1": {
          "swarm": {
            "constraints": [
              "node.role==leader"
            ]
          },
          "portMap": {
            "7050": 8050
          }
        },
        "orderer2": {
          "swarm": {
            "constraints": [
              "node.role==leader"
            ]
          },
          "portMap": {
            "7050": 9050
          }
        }
      }
    },
    "MSP": {
      "name": "OrdererMSPName",
      "id": "OrdererMSP"
    }
  },
  "channels": {
    "allChannel": {
      "file": "all.tx",
      "eventWaitTime": 30000,
      "orgs": {
        "SUPPLY": {
          "peerIndexes": [
            0
          ]
        },
        "CAR": {
          "peerIndexes": [
            0
          ]
        },
        "FACTORY": {
          "peerIndexes": [
            0
          ]
        },
        "TK": {
          "peerIndexes": [
            0
          ]
        }
      }
    }
  },
  "orgs": {
    "SUPPLY": {
      "peers": [
        {
          "swarm": {
            "constraints": [
              "node.role==manager"
            ]
          },
          "container_name": "SupplyChain",
          "portMap": [
            {
              "host": 7051,
              "container": 7051
            },
            {
              "host": 7053,
              "container": 7053
            }
          ]
        }
      ],
      "userCount": 0,
      "ca": {
        "enable": true,
        "admin": {
          "name": "CAadmin",
          "pass": "passwd"
        },
        "portHost": 7054,
        "tlsca": {
          "portHost": 7055
        }
      },
      "MSP": {
        "name": "SUPPLYMSPName",
        "id": "SUPPLYMSP"
      }
    },
    "CAR": {
      "peers": [
        {
