{
  "delphi": {
    "domain": "Delphi.com",
    "TLS": true,
    "docker": {
      "fabricTag": "1.1.0-preview",
      "network": "delphiNetwork",
      "volumes": {
        "CONFIGTX": {
          "local": "CONFIGTX_DIR",
          "swarm": "CONFIGTX_swarm",
          "dir": "/home/david/Documents/delphi-fabric/config/configtx"
        },
        "MSPROOT": {
          "local": "MSPROOT",
          "swarm": "MSPROOT_swarm",
          "dir": "/home/david/Documents/delphi-fabric/config/crypto-config/"
        }
      }
    },
    "orderer": {
      "containerName": "orderContainerName",
      "portMap": [
        {
          "host": 7250,
          "container": 7050
        }
      ],
      "MSP": {
        "name": "OrdererMSPName",
        "id": "OrdererMSP"
      }
    },
    "channels": {
      "delphiChannel": {
        "file": "delphi.tx",
        "eventWaitTime": 30000,
        "orgs": {
          "BU": {
            "peerIndexes": [
              0,
              1
            ]
          },
          "PM": {
            "peerIndexes": [
              0
            ]
          },
          "ENG": {
            "peerIndexes": [
              0
            ]
          }
        }
      },
      "BUChannel": {
        "eventWaitTime": 30000,
        "file": "bu.tx",
        "orgs": {
          "BU": {
            "peerIndexes": [
              0,
              1
            ]
          }
        }
      }
    },
    "orgs": {
      "BU": {
        "peers": [
          {
            "swarm": {
              "constraints": [
                "node.hostname == ubuntu"
              ]
            },
            "containerName": "BUContainerName",
            "portMap": [
              {
                "host": 7051,
                "container": 7051
              },
              {
                "host": 7053,
                "container": 7053
              }
            ]
          },
          {
            "swarm": {
              "constraints": [
                "node.hostname == fabric-swarm-manager"
              ]
            },
            "containerName": "BUContainer1",
            "portMap": [
              {
                "host": 7061,
                "container": 7051
              },
              {
                "host": 7063,
                "container": 7053
              }
            ]
          }
        ],
        "userCount": 0,
        "ca": {
          "enable": false,
          "containerName": "BUCA",
          "admin": {
            "name": "CAadmin",
            "pass": "passwd"
          },
          "portHost": 7054,
          "tlsca": {
            "containerName": "BUTLSCA",
            "portHost": 7055
          }
        },
        "MSP": {
          "name": "BUMSPName",
          "id": "BUMSP"
        }
      },
      "ENG": {
        "peers": [
          {
            "swarm": {
              "constraints": [
                "node.hostname == fabric-swarm-manager"
              ]
            },
            "containerName": "ENGContainerName",
            "portMap": [
              {
                "host": 8051,
                "container": 7051
              },
              {
                "host": 8053,
                "container": 7053
              }
            ]
          }
        ],
        "userCount": 0,
        "ca": {
          "enable": false,
          "containerName": "ENGCA",
          "admin": {
            "name": "CAadmin",
            "pass": "passwd"
          },
          "portHost": 8054,
          "tlsca": {
            "containerName": "ENGTLSCA",
            "portHost": 8055
          }
        },
        "MSP": {
          "name": "ENGMSPName",
          "id": "ENGMSP"
        }
      },
      "PM": {
        "peers": [
          {
            "swarm": {
              "constraints": [
                "node.hostname == ubuntu"
              ]
            },
            "containerName": "PMContainerName",
            "portMap": [
              {
                "host": 9051,
                "container": 7051
              },
              {
                "host": 9053,
                "container": 7053
              }
            ]
          }
        ],
        "userCount": 0,
        "ca": {
          "containerName": "PMCA",
          "enable": false,
          "admin": {
            "name": "CAadmin",
            "pass": "passwd"
          },
          "portHost": 9054,
          "tlsca": {
            "containerName": "PMTLSCA",
            "portHost": 9055
          }
        },
        "MSP": {
          "name": "PMMSPName",
          "id": "PMMSP"
        }
      }
    }
  }
}
